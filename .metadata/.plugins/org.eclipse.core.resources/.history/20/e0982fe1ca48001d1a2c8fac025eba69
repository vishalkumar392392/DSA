package com.array.problem;

public class SearchForRange {
	
	public static void main(String[] args) {
		
		
		int arr[] = {5, 7, 7, 8, 8, 10};
		int k = 8;
		
		int p1 = 0;
		int p2 = arr.length;
		
		while(p1<p2) {
			int mid = (p1+p2)/2;
			if(arr[mid] == k) {
				System.out.println("in mid");
				return;
				
			}if(arr[mid] < k) p1 = mid +1;
			if(arr[mid] > k) p2 = mid -1;
		}
	}

}
